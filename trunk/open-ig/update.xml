	<!-- Update definitions for the Open Imperium Galactica components -->
	<module id='Launcher' version='0.37'>
		<!-- General module description -->
		<general url='https://code.google.com/p/open-ig/wiki/Launcher'>
			<desc lang='hu'>
				Az Open Imperium Galactica indító programja, amely automatikusan letölti
				a különféle modulok fájljait telepítés, frissítés vagy javítás céljából.
			</desc>
			<desc lang='en'>
				The launcher for the Open Imperium Galactica which can download various module files
				for install, upgrade or repair purposes.
			</desc>
		</general>
		<!-- Release notes -->
		<notes url='https://code.google.com/p/open-ig/wiki/Launcher_Notes'>
			<desc lang='hu'>Francia nyelv támogatás.</desc>
			<desc lang='en'>French language support.</desc>
			<desc lang='de'>Französische Sprache möglichkeit.</desc>
			<desc lang='fr'>French language support (translation needed).</desc>
		</notes>
		<execute file='open-ig-launcher.jar' memory='64'/>
		<!-- Particular files location and hashes, file descriptions -->
		<!--  https://open-ig.googlecode.com/files/ -->
		<file url='https://open-ig.googlecode.com/svn/trunk/open-ig/open-ig-launcher.jar' sha1='110445d6d165906afffd8bcb3005e440b020418a'>
			<desc lang='hu'>Az indító fő fájlja</desc>
			<desc lang='en'>The launcher's main file</desc>
		</file>
	</module>
