  <module id='Launcher' version='0.43'>
    <general url='https://code.google.com/p/open-ig/wiki/Launcher'>
      <desc lang='hu'>
				Az Open Imperium Galactica indító programja, amely automatikusan letölti
				a különféle modulok fájljait telepítés, frissítés vagy javítás céljából.</desc>
      <desc lang='en'>
				The launcher for the Open Imperium Galactica which can download various module files
				for install, upgrade or repair purposes.</desc>
    </general>
    <notes url='https://code.google.com/p/open-ig/wiki/Launcher_Notes'>
      <desc lang='hu'>Spanyol nyelvi támogatás.</desc>
      <desc lang='en'>Spanish language support.</desc>
      <desc lang='de'>Spanish language support.</desc>
      <desc lang='fr'>Spanish language support.</desc>
      <desc lang='ru'>Spanish language support.</desc>
      <desc lang='es'>Spanish language support.</desc>
    </notes>
    <execute file='open-ig-launcher.jar' memory='64'/>
    <file url='https://open-ig.googlecode.com/svn/trunk/open-ig/open-ig-launcher.jar' sha1='AD466C1FFA6CFF24ADAE344E662B3A3842B73AD6'>
      <desc lang='hu'>Az indító fő fájlja</desc>
      <desc lang='en'>The launcher&#39;s main file</desc>
    </file>
  </module>
